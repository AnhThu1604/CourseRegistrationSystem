<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="pojo.PhieuDkEntity" table="phieuDK" schema="CourseRegistrationSystem">
        <composite-id mapped="true" class="pojo.PhieuDkEntityPK">
            <key-property name="maSv">
                <column name="MaSV" sql-type="varchar(50)" length="50"/>
            </key-property>
            <key-property name="maHocPhan">
                <column name="MaHocPhan" sql-type="varchar(30)" length="30"/>
            </key-property>
        </composite-id>
    </class>
</hibernate-mapping>