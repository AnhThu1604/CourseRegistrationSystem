<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class catalog="" name="pojo.GiaoVu" table="giaovu">
        <id name="ma" type="string">
            <column length="30" name="Ma"/>
            <generator class="assigned"/>
        </id>
        <property name="hoVaTen" type="string">
            <column length="50" name="HoVaTen" not-null="true"/>
        </property>
        <property name="phai" type="string">
            <column length="5" name="Phai" not-null="true"/>
        </property>
        <property name="ngaySinh" type="date">
            <column length="10" name="NgaySinh" not-null="true"/>
        </property>
        <property name="diaChi" type="string">
            <column length="50" name="DiaChi" not-null="true"/>
        </property>
        <property name="CMND" type="string">
            <column length="15" name="CMND" not-null="true"/>
        </property>
        <property name="matKhau" type="string">
            <column length="20" name="MatKhau" not-null="true"/>
        </property>
        <property name="email" type="string">
            <column length="50" name="Email" not-null="true"/>
        </property>
        <property name="maQuyenDN" type="int">
            <column length="5" name="MaQuyenDN" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>