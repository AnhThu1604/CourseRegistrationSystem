<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class catalog="" name="pojo.HocPhan"
           table="hocphan">
        <id name="maHocPhan" type="string">
            <column length="30" name="MaHocPhan"/>
            <generator class="assigned"/>
        </id>
        <property name="maMonHoc" type="string">
            <column length="30" name="MaMonHoc" not-null="true"/>
        </property>
        <property name="tenMonHoc" type="string">
            <column length="50" name="TenMonHoc" not-null="true"/>
        </property>
        <property name="soTinChi" type="int">
            <column length="5" name="SoTinChi" not-null="true"/>
        </property>
        <property name="GVLT" type="string">
            <column length="100" name="GVLT" not-null="true"/>
        </property>
        <property name="tenPhongHoc" type="string">
            <column length="30" name="TenPhongHoc" not-null="true"/>
        </property>
        <property name="ngayHoc" type="int">
            <column length="5" name="NgayHoc" not-null="true"/>
        </property>
        <property name="caHoc" type="int">
            <column length="5" name="CaHoc" not-null="true"/>
        </property>
        <property name="slotToiDa" type="int">
            <column length="5" name="SlotToiDa" not-null="true"/>
        </property>
        <property name="maLopHoc" type="string">
            <column length="30" name="MaLopHoc" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>