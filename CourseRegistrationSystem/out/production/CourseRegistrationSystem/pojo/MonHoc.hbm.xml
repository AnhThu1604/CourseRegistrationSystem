<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class catalog="" name="pojo.MonHoc" table="monhoc">
        <id name="maMonHoc" type="string">
            <column length="30" name="MaMonHoc"/>
            <generator class="assigned"/>
        </id>
        <property name="tenMonHoc" type="string">
            <column length="50" name="TenMonHoc" not-null="true"/>
        </property>
        <property name="soTinChi" type="int">
            <column length="5" name="SoTinChi" not-null="true"/>
        </property>
        <property name="maHocKi" type="int">
            <column length="10" name="MaHocKi" not-null="true"/>
        </property>
        <many-to-one class="pojo.HocPhan" name="maMonHoc"
                     fetch="select">
            <column length="30" name="MaMonHoc" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>