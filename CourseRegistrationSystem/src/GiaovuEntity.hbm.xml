<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="pojo.GiaovuEntity" table="giaovu" schema="CourseRegistrationSystem">
        <id name="ma">
            <column name="Ma" sql-type="varchar(30)" length="30"/>
        </id>
        <property name="hoVaTen">
            <column name="HoVaTen" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="phai">
            <column name="Phai" sql-type="varchar(5)" length="5"/>
        </property>
        <property name="ngaySinh">
            <column name="NgaySinh" sql-type="date"/>
        </property>
        <property name="diaChi">
            <column name="DiaChi" sql-type="varchar(255)"/>
        </property>
        <property name="cmnd">
            <column name="CMND" sql-type="varchar(12)" length="12" not-null="true"/>
        </property>
        <property name="matKhau">
            <column name="MatKhau" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="email">
            <column name="Email" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="maQuyenDn">
            <column name="MaQuyenDN" sql-type="int"/>
        </property>
    </class>
</hibernate-mapping>