<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="pojo.DangkiHpEntity" table="dangkiHP" schema="CourseRegistrationSystem">
        <id name="maDkhp">
            <column name="MaDKHP" sql-type="int"/>
        </id>
        <property name="ngayBatDau">
            <column name="NgayBatDau" sql-type="datetime" not-null="true"/>
        </property>
        <property name="ngayKetThuc">
            <column name="NgayKetThuc" sql-type="datetime" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>